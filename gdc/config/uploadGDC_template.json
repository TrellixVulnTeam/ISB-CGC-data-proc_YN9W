{
    "mode": "test",
    "threads": 40,
    "upload_files": false,
    "upload_etl_files": false,
    "upload_open": false,
    "upload_controlled": true,
    "program_names": [
        "TCGA",
        "TARGET"
    ],
    "project_names": [
        "all"
    ],
    "project_names_test": [
        "TCGA-ACC",
        "TCGA-UCS",
        "TCGA-DLBC",
        "TCGA-LUSC",
        "TCGA-PRAD",
        "TCGA-KICH",
        "TCGA-PAAD",
        "TCGA-CHOL"
    ],
    "data_types": [
        "Copy Number Segment", 
        "Annotated Somatic Mutation", 
        "Masked Copy Number Segment", 
        "miRNA Expression Quantification", 
        "Aligned Reads", 
        "Isoform Expression Quantification", 
        "Gene Expression Quantification", 
        "Raw Simple Somatic Mutation",
        "Biospecimen",
        "Clinical"
    ],
    "data_categories": [
        "Simple Nucleotide Variation",
        "Transcriptome Profiling",
        "Raw Sequencing Data",
        "Copy Number Variation",
        "Biospecimen",
        "Clinical"
    ],
    "category2type": {
        "Simple Nucleotide Variation": ["Annotated Somatic Mutation", "Raw Simple Somatic Mutation"],
        "Transcriptome Profiling": ["miRNA Expression Quantification", "Isoform Expression Quantification", "Gene Expression Quantification"],
        "Raw Sequencing Data": ["Aligned Reads"],
        "Copy Number Variation": ["Copy Number Segment", "Masked Copy Number Segment"],
        "Biospecimen": ["Biospecimen"],
        "Clinical": ["Clinical"]
    },
    "metadata_modules": [
        "store_metadata_cloudsql"
    ],
    "projects_endpt": {
        "endpt": "https://gdc-api.nci.nih.gov/projects",
        "query": "?expand=program,summary,summary.experimental_strategies,summary.data_categories&pretty=true"
    },
    "annotations_endpt": {
        "endpt": "https://gdc-api.nci.nih.gov/annotations",
        "query": ""
    },
    "cases_endpt": {
        "endpt": "https://gdc-api.nci.nih.gov/cases",
        "query": "?expand=annotations,project,project.program,samples,samples.annotations,samples.portions,samples.portions.center,samples.portions.analytes,samples.portions.analytes.aliquots,samples.portions.analytes.aliquots.center,samples.portions.analytes.aliquots.annotation&pretty=true"
    },
    "files_endpt": {
        "endpt": "https://gdc-api.nci.nih.gov/files",
        "query": "?expand=archive,cases,cases.project,cases.project.program,cases.samples,cases.samples.portions.analytes.aliquots,center&pretty=true"
    },
    "database_module": "gdc.model.isbcgc_cloudsql_gdc_model",
    "cloudsql": {
        "host": "xxx.xxx.xxx.xxx",
        "db": "db",
        "user": "user",
        "passwd": "passwd",
        "update_schema": false,
        "ssl_dir": "../../ssl/"
    },
    "log_dir_tag": "run",
    "process_annotations": {
        "call_map_rows": true,
        "fetch_count": 1000,
        "annotation_fetch_size": 1000,
        "filter_result": {
            "value": {
                "status": "status",
                "category": "annotationCategoryName",
                "entity_id": "itemTypeId",
                "classification": "annotationClassification",
                "updated_datetime": "dateEdited",
                "created_datetime": "dateCreated",
                "annotation_id": "annotationId",
                "notes": "annotationNoteText",
                "entity_type": "itemTypeName",
                "entity_submitter_id": "itemBarcode",
                "case_submitter_id": "ParticipantBarcode"
            }
        },
        "annotation_table_mapping": {
            "value": {
                "status": "status",
                "annotationCategoryName": "annotationCategoryName",
                "itemTypeId": "itemTypeId",
                "annotationClassification": "annotationClassification",
                "dateEdited": "substr,0,19,dateEdited",
                "dateCreated": "substr,0,19,dateCreated",
                "annotationId": "annotationId",
                "annotationNoteText": "annotationNoteText",
                "itemTypeName": "itemTypeName",
                "itemBarcode": "itemBarcode",
                "ParticipantBarcode": "ParticipantBarcode"
            }
        }
    },
    "process_cases": {
        "case_fetch_size": 1000,
        "fetch_count": 20,
        "filter_result": {
            "value": {
                "submitter_id": "tcga_barcode", 
                "case_id": "case_id"
            },
            "map": {
                "project": {
                    "project": "project",
                    "value": {
                        "project_id": "project_id"
                    },
                    "map": {
                        "program": {
                            "program": "program",
                            "value": {
                                "name": "program_name"
                            }
                        }
                    }
                }
            },
            "map_list": {
                "samples": {
                    "samples": "samples",
                    "value": {
                        "submitter_id": "tcga_barcode", 
                        "sample_id": "sample_id", 
                        "is_ffpe": "is_ffpe" 
                    }
                }
            }
        },
        "clinical_table_mapping": {
            "value": {
                "tcga_barcode": "ParticipantBarcode", 
                "case_id": "ParticipantUUID"
            },
            "map": {
                "project": {
                    "project": "project",
                    "value": {
                        "project_id": "Project"
                    },
                    "map": {
                        "program": {
                            "program": "program",
                            "value": {
                                "program_name": "Program"
                            }
                        }
                    }
                }
            }
        },
        "sample_table_mapping": {
            "value": {
                "tcga_barcode": "ParticipantBarcode"
            },
            "map": {
                "project": {
                    "project": "project",
                    "value": {
                        "project_id": "Project"
                    },
                    "map": {
                        "program": {
                            "program": "program",
                            "value": {
                                "program_name": "Program"
                            }
                        }
                    }
                }
            },
            "map_list": {
                "samples": {
                    "samples": "samples",
                    "value": {
                        "tcga_barcode": "SampleBarcode", 
                        "sample_id": "SampleUUID"
                    }
                }
            }
        }
    },
    "process_files": {
        "call_map_rows": true,
        "fetch_count": 20,
        "file_fetch_size": 1000,
        "filter_result": {
            "value": {
                "acl": "acl",
                "file_id": "file_id",
                "data_type": "data_type", 
                "file_name": "file_name", 
                "md5sum": "md5sum", 
                "data_format": "data_format", 
                "access": "access", 
                "state": "state", 
                "data_category": "data_category", 
                "file_size": "filesize", 
                "type": "type", 
                "file_state": "file_state", 
                "experimental_strategy": "experimental_strategy"
            },
            "map_list": {
                "cases": {
                    "cases": "cases",
                    "value": {
                        "submitter_id": "tcga_barcode", 
                        "case_id": "case_id"
                    },
                    "map": {
                        "project": {
                            "project": "project",
                            "value": {
                                "project_id": "project_id"
                            },
                            "map": {
                                "program": {
                                    "program": "program",
                                    "value": {
                                        "name": "program_name"
                                    }
                                }
                            }
                        }
                    },
                    "map_list": {
                        "samples": {
                            "samples": "samples",
                            "value": {
                                "submitter_id": "tcga_barcode"
                            },
                            "map_list": {
                                "portions": {
                                    "portions": "portions",
                                    "map_list": {
                                        "analytes": {
                                            "analytes": "analytes",
                                            "map_list": {
                                                "aliquots": {
                                                    "aliquots": "aliquots",
                                                    "value": {
                                                        "submitter_id": "tcga_barcode", 
                                                        "aliquot_id": "aliquot_id" 
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "data_table_mapping": {
            "value": {
                "data_type": "DataType", 
                "file_name": "FileName", 
                "md5sum": "md5sum", 
                "data_format": "DataFormat", 
                "access": "access", 
                "state": "state", 
                "data_category": "DataCategory", 
                "filesize": "filesize", 
                "file_state": "file_state", 
                "experimental_strategy": "ExperimentalStrategy"
            },
            "map_list": {
                "cases": {
                    "cases": "cases",
                    "value": {
                        "tcga_barcode": "ParticipantBarcode"
                    },
                    "map": {
                        "project": {
                            "project": "project",
                            "value": {
                                "project_id": "Project"
                            },
                            "map": {
                                "program": {
                                    "program": "program",
                                    "value": {
                                        "program_name": "Program_name"
                                    }
                                }
                            }
                        }
                    },
                    "map_list": {
                        "samples": {
                            "samples": "samples",
                            "value": {
                                "tcga_barcode": "SampleBarcode"
                            },
                            "map_list": {
                                "portions": {
                                    "portions": "portions",
                                    "map_list": {
                                        "analytes": {
                                            "analytes": "analytes",
                                            "map_list": {
                                                "aliquots": {
                                                    "aliquots": "aliquots",
                                                    "value": {
                                                        "tcga_barcode": "AliquotBarcode", 
                                                        "aliquot_id": "AliquotUUID" 
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}
